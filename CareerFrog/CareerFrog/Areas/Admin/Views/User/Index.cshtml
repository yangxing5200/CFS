    <script type="text/javascript">
        (function ($) {
            $().ready(function () {
                var first = 0;
             
                var needPagging = true;
              
                var pageInit = function (page, size, param) {
                    //加载数据
                    var index = 0;
                    $.ajax({
                        url: "/api/CommonApi",
                        type: "get",
                        dataType: "json",
                        data: {
                            API: "pagedquery",
                            param: '',
                            sorts: ' CreateTime Desc',
                            pagesize: size,
                            page: page
                        },
                        beforeSend: function (xhr) {
                            index = layer.load('正在处理...');
                        },
                        success: function (xhr) {
                            layer.close(index);
                            var html = template('trs', xhr);
                            $('#dataList').empty().append(html);

                            if (first == 0) {
                                //总数目
                                var length = xhr.ApiParamObj[1][0].totalRow;
                                $("#Pagination").pagination(length, {
                                    num_edge_entries: 2,
                                    num_display_entries: 4,
                                    callback: pageselectCallback,
                                    items_per_page: 1
                                });

                                //从表单获取分页元素参数
                                var optInit = getOptionsFromForm();
                                $("#Pagination").pagination(length, optInit);


                                //-----------------------------------
                                function getOptionsFromForm() {
                                    var opt = { callback: pageselectCallback, prev_text: "上一页", next_text: "下一页" };

                                    opt.num_edge_entries = 2;
                                    opt.num_display_entries = 5;
                                    opt.items_per_page = 10;
                                    return opt;
                                }

                                //-------------------------------
                                function pageselectCallback(page_index, jq) {
                                    //自动加载取消
                                    if (first > 1) {
                                        pageInit(page_index + 1, 10, $('#q-param').val());
                                    }
                                    first++;
                                    //阻止单击事件
                                    return false;
                                }
                            }
                        }
                    });
                }
                pageInit(1, 10, '');
            });
           
        })(jQuery);
  
        setInterval(function () { $("[jason='good']").fadeOut(100).fadeIn(100); }, 200);
